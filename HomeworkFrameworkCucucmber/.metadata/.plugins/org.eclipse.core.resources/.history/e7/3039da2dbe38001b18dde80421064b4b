	@BeforeTest
	public void start()
	{
	launchApplication("safari","https://opensource-demo.orangehrmlive.com/");
		}
	@Test
	public void login()
	{
		OrangeHrmLogin ohrm = new OrangeHrmLogin(driver);
		ohrm.loginToApp_username("Admin");
